#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../convert-bootstrap-2-to-3/bin/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../convert-bootstrap-2-to-3/bin/index.js" "$@"
  ret=$?
fi
exit $ret
