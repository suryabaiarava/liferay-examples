.modal-header .close{margin-top: 5px;/*background-color: #d44343;*/opacity: 2;padding: 10px;/*border-radius: 100%;border: 2px solid #fff;*/ line-height: 13px;color: #000; margin-right: 7px;}
.effect-for-read-unread-tabs{background-color: skyblue;} 

@media(max-width:1024px){.header-icons .fa-tasks, .header-icons .fa-bell-o{margin-top:11px; }}
